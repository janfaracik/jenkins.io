%h3{:id => "v#{page.release.version}", :class => 'idkanymore' }
  %a{:href => "/changelog-stable/#{page.release.version}/"}
    = page.release.version
- if page.release.date
  %div{:class => 'notthecrowd'}
    = page.release.date.strftime('%B %-d, %Y')
- if page.release.banner
  %div{:style => 'margin: 10px; padding: 10px; background-color: #FFFFCE;'}
    = page.release.banner
